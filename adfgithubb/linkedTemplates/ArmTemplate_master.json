{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adfgithubb"
		},
		"AzureBlobStorage1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage1'"
		},
		"AzureBlobStorage2_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage2'"
		},
		"ParamFolderBlobLink_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ParamFolderBlobLink'"
		},
		"SQLServer_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'SQLServer'"
		},
		"SQLServer_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "192.168.31.217\\CHUKKASQLSERVER"
		},
		"SQLServer_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "@{linkedService().DataBaseName}"
		},
		"SQLServer_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "sa"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adfgithubb_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobStorage1_connectionString": {
						"value": "[parameters('AzureBlobStorage1_connectionString')]"
					},
					"AzureBlobStorage2_connectionString": {
						"value": "[parameters('AzureBlobStorage2_connectionString')]"
					},
					"ParamFolderBlobLink_connectionString": {
						"value": "[parameters('ParamFolderBlobLink_connectionString')]"
					},
					"SQLServer_password": {
						"value": "[parameters('SQLServer_password')]"
					},
					"SQLServer_properties_typeProperties_server": {
						"value": "[parameters('SQLServer_properties_typeProperties_server')]"
					},
					"SQLServer_properties_typeProperties_database": {
						"value": "[parameters('SQLServer_properties_typeProperties_database')]"
					},
					"SQLServer_properties_typeProperties_userName": {
						"value": "[parameters('SQLServer_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}